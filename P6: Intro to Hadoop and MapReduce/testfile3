"1010299"	""	"cs373 "	"100010556"	"<h1>scheme implementation</h1>
<p>this is my second version which is a bit longer than the original, but handles arbitrarily nested structures through use of <code>enumerated</code></p>
<pre><code>(define (transpose . l) (apply map list l))
(define (partial f . a) (lambda x (apply f (append a x))))
(define (flatten lst)
  (cond ((null? lst) '())
        ((list? lst) 
         (append 
           (flatten (car lst))
           (flatten (cdr lst))))
        (else (list lst))))
(define (foldl f a lst)
  (if (null? lst) a
    (foldl f (f a (car lst)) (cdr lst))))
(define dimensional-ref (partial foldl list-ref))
(define (range n)
  (let loop ((start 0) (r '()))
    (if (&gt;= start n)
      (reverse r)
      (loop (+ start 1) (cons start r)))))

(define (enumerated p f)
  (define (inner c e i)
    (if (list? e)
      (map 
        (partial apply inner (cons i c))
        (transpose e (range (length e))))
      (f (reverse (cons i c)))))
  (map 
    (partial apply inner '())
    (transpose p (range (length p)))))

(define (normalize p)
  (define (sub indices)
    (/ (dimensional-ref p indices) 
       (apply + (flatten p))))
  (enumerated p sub))

(define colors
  '((r g g r r)
    (r r g r r)
    (r r g g r)
    (r r r r r)))
(define measures
  '(g g g g g))
(define motions
  '((0 0)
    (0 1)
    (1 0)
    (1 0)
    (0 1)))
(define sensor_right 7/10)
(define p_move 4/5)
(define p (normalize (enumerated colors (lambda x 1))))

(define (sense p Z)
  (define (hit? indices)
    (* (if (eq? Z (dimensional-ref colors indices))
         sensor_right
         (- 1 sensor_right))
       (dimensional-ref p indices)))
  (normalize (enumerated p hit?)))

(define (move p d-indices)
  (define (move-each indices)
    (define (access parent di i)
      (list-ref parent (modulo (- i di) (length parent))))
    (+ (* p_move 
          (foldl (partial apply access) p (transpose d-indices indices)))
       (* (- 1 p_move) (dimensional-ref p indices))))
  (normalize (enumerated p move-each)))

(define (step p motion measure)
  (sense (move p motion) measure))

(foldl (partial apply step) p (transpose motions measures))
</code></pre>"	"answer"	"1008404"	"1008404"	"2012-03-02 06:18:03.874379+00"	"0"	""	"1041692"	"100010556"	"2012-03-02 19:35:21.409192+00"	"1014751"	"\N"	"\N"	"0"	"f"
